{
  "name": "@omh/react-native-maps",
  "version": "1.0.0",
  "description": "React Native OMH Maps Workspace",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/runtime": "^7.24.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@evilmartians/lefthook": "^1.6.7",
    "@react-native/babel-preset": "0.75.0-main",
    "@react-native/eslint-config": "^0.75.0-main",
    "@react-native/metro-config": "0.75.0-main",
    "@react-native/typescript-config": "0.75.0-main",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@types/prompts": "^2.4.9",
    "@types/react": "^18.2.67",
    "babel-plugin-module-resolver": "^5.0.0",
    "commitlint": "^19.2.1",
    "cross-env": "^7.0.3",
    "del-cli": "^5.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "ora": "^8.0.1",
    "prettier": "^3.2.5",
    "prompts": "^2.4.2",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-builder-bob": "^0.23.2",
    "release-it": "^17.1.1",
    "tsx": "^4.7.1",
    "turbo": "^1.12.5",
    "typescript": "^5.4.3"
  },
  "scripts": {
    "sample-app": "cross-env FORCE_COLOR=1 yarn workspace @omh/react-native-maps-sample-app",
    "scripts": "cross-env FORCE_COLOR=1 yarn workspace @omh/react-native-maps-scripts",
    "core": "cross-env FORCE_COLOR=1 yarn workspace @omh/react-native-maps-core",
    "build:packages": "yarn run prepare",
    "build:android": "npx turbo build:android",
    "build:ios": "npx turbo build:ios",
    "test": "cross-env FORCE_COLOR=1 yarn workspaces foreach -Ap run test",
    "typecheck": "cross-env FORCE_COLOR=1 yarn workspaces foreach -p --exclude @omh/react-native-maps-sample-app run typecheck",
    "lint": "cross-env FORCE_COLOR=1 yarn workspaces foreach -p --exclude @omh/react-native-maps-sample-app run lint",
    "clean": "cross-env FORCE_COLOR=1 yarn workspaces foreach -p --exclude @omh/react-native-maps-sample-app run clean",
    "prepare": "cross-env FORCE_COLOR=1 yarn workspaces foreach -p --exclude @omh/react-native-maps-sample-app run prepare",
    "release": "cross-env FORCE_COLOR=1 yarn workspaces foreach -p --exclude @omh/react-native-maps-sample-app run release"
  },
  "resolutions": {
    "@types/react": "^18.2.44"
  },
  "workspaces": [
    "sample-app",
    "packages/*",
    "scripts"
  ],
  "packageManager": "yarn@3.6.1",
  "dependencies": {
    "dedent": "^1.5.1"
  }
}
