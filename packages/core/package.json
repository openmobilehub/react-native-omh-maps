{
  "name": "@openmobilehub/maps-core",
  "version": "1.0.1",
  "description": "React Native OMH Maps Core",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "clean": "del-cli lib",
    "prepare": "bob build",
    "release": "release-it"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openmobilehub/react-native-omh-maps.git"
  },
  "author": "OpenMobileHub <contact@openmobilehub.com> (https://github.com/openmobilehub)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/openmobilehub/react-native-omh-maps/issues"
  },
  "homepage": "https://github.com/openmobilehub/react-native-omh-maps#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "@react-native-community/geolocation": ">=3.0.0",
    "react": "*",
    "react-native": "*",
    "react-native-maps": ">=1.15.0"
  },
  "packageManager": "yarn@3.6.1",
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "codegenConfig": {
    "name": "RNOmhMaps",
    "type": "all",
    "jsSrcsDir": "src",
    "android": {
      "javaPackageName": "com.openmobilehub.android.rn.maps.core"
    }
  },
  "devDependencies": {
    "@react-native-community/geolocation": "^3.2.1",
    "@types/node": "*",
    "del-cli": "*",
    "jest": "*",
    "react-native-builder-bob": "*",
    "react-native-maps": "^1.15.1",
    "release-it": "*",
    "ts-node": "*",
    "typescript": "*"
  }
}
